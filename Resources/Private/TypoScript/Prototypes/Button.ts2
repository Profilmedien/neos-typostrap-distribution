prototype(M12.Foundation:Button) < prototype(M12.Foundation:Content) {
	attributes.class.size = ${q(node).property('classSize') ? q(node).property('classSize') : null}
	attributes.class.type = ${q(node).property('classType') ? q(node).property('classType') : null}
	attributes.class.shape = ${q(node).property('classShape') ? q(node).property('classShape') : null}
	attributes.class.disable = ${q(node).property('isDisabled') ? 'disabled' : null}
	attributes.class.expand = ${q(node).property('isExpanded') ? 'expand' : null}

	tag = 'button'
}
